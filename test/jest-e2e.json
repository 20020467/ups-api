{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/s3-upload/(.*)": "<rootDir>/../libs/s3-upload/src/$1",
    "@app/s3-upload": "<rootDir>/../libs/s3-upload/src",
    "@app/jwt-authentication/(.*)": "<rootDir>/../libs/jwt-authentication/src/$1",
    "@app/jwt-authentication": "<rootDir>/../libs/jwt-authentication/src"
  }
}